{
  "kind": "collectionType",
  "collectionName": "exams",
  "info": {
    "singularName": "exam",
    "pluralName": "exams",
    "displayName": "Exam",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "date": {
      "type": "datetime",
      "required": true
    },
    "duration": {
      "type": "integer",
      "required": true
    },
    "student": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::student.student"
    },
    "tutor": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::tutor.tutor"
    },
    "examiner": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::examiner.examiner",
      "inversedBy": "exams"
    },
    "notes": {
      "type": "string"
    },
    "exam_mode": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::exam-mode.exam-mode"
    },
    "institute": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::institute.institute"
    },
    "room": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::room.room",
      "inversedBy": "exams"
    },
    "confirmed": {
      "type": "boolean",
      "default": false
    },
    "registeredTutors": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tutor.tutor"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "Email Examiner Needed",
        "Material Needed",
        "No Tutor Needed",
        "No Action Required",
        "Archived"
      ],
      "default": "Email Examiner Needed",
      "required": false
    },
    "lva_num": {
      "type": "string",
      "regex": "^[0-9]+([.][0-9]+)?$",
      "required": true
    }
  }
}
